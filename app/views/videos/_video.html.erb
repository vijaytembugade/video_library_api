<div id="<%= dom_id video %>">
  <p>
    <strong>Title:</strong>
    <%= video.title %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= video.category %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= video.url %>
  </p>

  <p>
    <strong>Thumbnail:</strong>
    <%= video.thumbnail %>
  </p>

  <p>
    <strong>Channelname:</strong>
    <%= video.channelName %>
